class Solution {
public:
    int findTheWinner(int n, int k) {
        queue<int>q;
        for(int i=1;i<=n;i++)
        q.push(i);

        while(q.size()>1){
            int count=k;
            while(count>1){
                int temp=q.front();
                q.pop();
                q.push(temp);
                count--;
            }
            q.pop();

        }
        return q.front();
    }
};
